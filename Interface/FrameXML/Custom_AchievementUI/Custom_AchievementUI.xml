	<Frame name="CustomAchievementFrame">
		<Scripts>
			<OnLoad>
				self:RegisterEvent("ADDON_LOADED");
			</OnLoad>
			<OnEvent>
				if arg1 == "Blizzard_AchievementUI" then
					InitCustonAchievementUI();

					self:UnregisterEvent(event);
				end
			</OnEvent>
		</Scripts>
	</Frame>
