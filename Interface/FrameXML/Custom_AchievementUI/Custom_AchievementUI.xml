				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentTopLeftCorner2" x="4" y="-8"/>
					<Anchor point="BOTTOMRIGHT" relativeTo="$parentBottomRightCorner" x="-24" y="8"/>
