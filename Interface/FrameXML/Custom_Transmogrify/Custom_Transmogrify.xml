			</OnShow>
			<OnEvent function="TransmogrifySlotButton_OnEvent"/>
			<OnClick function="TransmogrifySlotButton_OnClick"/>
			<OnEnter>
				self:OnEnter()
			</OnEnter>
			<OnLeave>
				self:OnLeave()
			</OnLeave>
			<OnDragStart>
				TransmogrifySlotButton_OnClick(self, "LeftButton");
			</OnDragStart>
		</Scripts>
